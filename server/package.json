{
  "name": "iftta",
  "description": "If This Then Ad",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "node index.ts",
    "compile": "tsc",
    "pre-build": "rm -rf ../dist",
    "build": "tsc && npm run lint",
    "post-build": "cd ../dist && npm install",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "copy-static": "cp -R static/ ../dist/static/ && cp package* ../dist", 
    "build-prod": "npm run pre-build && npm run build && npm run copy-static && npm run post-build",
    "build-dist": "npm run pre-build && npm run compile && npm run copy-static && npm run post-build",
    "serve": "node ../dist/index.js"
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "author": "Google LLC",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.10",
    "express": "^4.17.1",
    "typescript": "^4.5.2"
  },
  "devDependencies": {
  }
}
