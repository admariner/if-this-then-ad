{
  "name": "iftta",
  "description": "If This Then Ad",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "compile": "npx tsc",
    "reset": "rm -Rf dist && rm -Rf node_modules",
    "build": "npx tsc && npm run lint",
    "post-build": "cd dist/ && npm install",
    "lint": "npx tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "copy-static": "cp -R public dist/ && cp package* dist/",
    "build-local": "npm install --production=false && npm run build && npm run copy-static && npm run post-build",
    "build-dist": "npm i typescript && npm run compile && npm run copy-static && npm run post-build",
    "w-typescript": "npx tsc -w",
    "w-node": "nodemon dist/index.js",
    "dev": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,green.bold\" \"npm run w-typescript\" \"npm run w-node\"",
    "test": "jest"
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "author": "Google LLC",
  "license": "Apache-2.0",
  "dependencies": {
    "@google-cloud/logging-winston": "^4.1.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "googleapis": "^91.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-google-oidc": "^0.1.0",
    "typescript": "^4.5.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.10",
    "@types/passport": "^1.0.7",
    "@types/passport-google-oauth2": "^0.1.4",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "concurrently": "^6.4.0",
    "eslint": "^8.3.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^27.3.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.0",
    "typescript": "^4.5.2"
  }
}
